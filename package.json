{
  "name": "vitest-react-mock",
  "version": "0.0.3",
  "description": "vitest helper for react-mock-component",
  "keywords": [
    "vitest",
    "react",
    "mock",
    "tdd",
    "testing",
    "expect"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/vitest-react-mock.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/vitest-react-mock",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --tsconfig tsconfig.build.json",
    "check-types": "attw --pack .",
    "prepublishOnly": "pnpm run build",
    "release": "standard-version",
    "lint": "eslint --ext ts,tsx,js .",
    "pretest": "tsc -p tsconfig.test.json --noEmit",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@vitest/expect": "~3.2.4",
    "expect": "~30.0.3",
    "jest-matcher-utils": "~30.0.3"
  },
  "peerDependencies": {
    "react-mock-component": "^3.0.0 || ^4.0.0",
    "vitest": "^3.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "~0.18.0",
    "@nighttrax/eslint-config-tsx": "~11.0.0",
    "@tdd-buffet/tsconfig": "~1.0.4",
    "@testing-library/react": "~16.1.0",
    "@types/react": "~18.3.12",
    "@vitejs/plugin-react": "~4.6.0",
    "eslint": "~8.57.0",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-mock-component": "~4.1.1",
    "standard-version": "~9.5.0",
    "strip-ansi": "~6.0.0",
    "typescript": "~4.9.0",
    "@vitest/coverage-v8": "~3.2.4",
    "tsup": "~8.5.0",
    "vitest": "~3.2.4"
  },
  "packageManager": "pnpm@9.14.4"
}
